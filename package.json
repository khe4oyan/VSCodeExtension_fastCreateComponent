{
  "name": "fomponent",
  "displayName": "Fomponent",
  "description": "Fomponent is a tool for instant creation of components in your project",
  "version": "1.1.0",
  "publisher": "khechoyan",
  "engines": {
    "vscode": "^1.84.2"
  },
  "author": {
    "name": "Khechoyan"
  },
  "categories": [
    "Other"
  ],
  "icon": "logo.png",
  "galleryBanner": {
    "color": "#00003D",
    "theme": "dark"
  },
  "activationEvents": [],
  "main": "./src/index.js",
  "contributes": {
    "commands": [
      {
        "command": "fomponent.createComponent",
        "title": "Create Component"
      },
      {
        "command": "fomponent.createMultiComponent",
        "title": "Create Multi Components"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "fomponent.createComponent",
          "group": "1_modification",
          "when": "resourceScheme == 'file' && explorerResourceIsFolder"
        },
        {
          "command": "fomponent.createMultiComponents",
          "group": "1_modification",
          "when": "resourceScheme == 'file' && explorerResourceIsFolder"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint"
  },
  "devDependencies": {
    "@types/vscode": "^1.84.2",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "eslint": "^8.57.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.8",
    "@vscode/test-electron": "^2.3.9"
  }
}
